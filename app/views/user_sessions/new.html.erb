<div id="big-title">
  <h1>FAKE-MM</h1>
  <p><strong>Your color</strong> Your style</p>
</div>

<div id="register-form">
<% form_for(@user_session) do |f| %>
  <%= error_messages_for :user_session %>
  
  <%= t('activerecord.errors.messages.equal_to', :count => '4') -%>
  

  <table>
    <tr>
      <td>
	<%= f.label t('activerecord.attributes.user.email') %><br />
	<%= f.text_field :email %>
      </td>
      <td>
	<%= f.label t('activerecord.attributes.user.password') %><br />
	<%= f.password_field :password %>
      </td>
    </tr>
    <tr>
      <td>
	<%= f.submit t('actions.login') %>
      </td>
      <td>
	<p><%= link_to t('actions.recover_password'), recover_password_path %></p>
      </td>
    </tr>
  </table>
<% end %>
</div

