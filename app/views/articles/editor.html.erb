<div>
  <div class="editor box">
    <h1><%= t('titles.editor') -%></h1>
   
    <%= will_paginate @articles -%>
    

    <ul>
      <% for article in @articles do %>
      <%= render article, :type => :editor_item %>
      <% end %>
    </ul>

  </div>

  <div class="clear"></div>
  <div class="box-footer-plain"></div>
</div>
